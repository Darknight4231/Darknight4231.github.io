{
"manifest_version": 2,
"name": "FastVideo",
"description": "This sets all Videos to play faster. Please access Options page before using.",
"version": "999",
"browser_action": {
   "default_icon": "Fishy2.png",
   "default_popup": "Popup2.html"
  },
  "background":{
    "scripts": ["Background5.js"],
    "persistent": true
  },
  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js": ["Content3.js"]
    }
  ],

"permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
